    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.40.0",
    "imagekit": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-svelte": "^0.525.0",
    "midtrans-client": "^1.4.3",
    "mysql2": "^3.12.0",
    "slugify": "^1.6.6",
    "svelte-toastify": "^0.0.27",
    "ulid": "^3.0.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
